{
  "dataset_reader": {
    "class_name": "dstc2_reader",
    "data_path": "{DOWNLOADS_PATH}/dstc2_v3"
  },
  "dataset_iterator": {
    "class_name": "dialog_db_result_iterator"
  },
  "chainer": {
    "in": ["db_result"],
    "in_y": [],
    "out": [],
    "pipe": [
      {
        "id": "restaurant_database",
        "class_name": "sqlite_database",
        "fit_on": ["db_result"],
        "table_name": "mytable",
        "primary_keys": ["name"],
        "save_path": "{DOWNLOADS_PATH}/dstc2_v3/resto.sqlite"
      }
    ]
  },
  "train": {
    "class_name": "fit_trainer",
    "evaluation_targets": [
      "valid",
      "test"
    ]
  },
  "metadata": {
    "variables": {
      "ROOT_PATH": "~/.deeppavlov",
      "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
      "MODELS_PATH": "{ROOT_PATH}/models"
    },
    "download": [
      {
        "url": "http://files.deeppavlov.ai/datasets/dstc2_v3.tar.gz",
        "subdir": "{DOWNLOADS_PATH}/dstc2_v3"
      }
    ]
  }
}
